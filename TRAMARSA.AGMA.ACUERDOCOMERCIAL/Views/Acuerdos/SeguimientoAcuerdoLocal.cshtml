                                    <div id="grillaListaSeguimientoACLocal_pager"></div>
                    
                                </ng-jqgrid>

                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <!-- RESULTADO BUSQUEDA -->
    </div>
</div>
