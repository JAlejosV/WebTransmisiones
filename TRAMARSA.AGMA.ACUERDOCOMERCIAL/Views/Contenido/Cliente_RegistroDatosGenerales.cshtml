<div>
    <div class="block_cab block_cab_active">
        <h2>
            <span>Datos Generales</span>
        </h2>
        <h3></h3>
        <div class="acordeon">
        </div>
    </div>
    <form id="RegistroDocumentoClientesFrm" name="RegistroDocumentoClientesFrm">
        <div class="block_content" style="width: 100%">
            <div class="col-md-12 row">
                <div class="col-md-3">
                    Rol
                </div>
                <div class="col-md-9">
                    <select ng-model="$root.DatosFormulario.RegistroDocumento.RegistroCliente.CodigoRol"
                            ng-options="rol.CodigoRol as rol.NombreRol for rol in $root.DatosFormulario.RegistroDocumento.RegistroCliente.Roles"
                            ng-change="LimpiarNombreRol()"
                            class="InputSELECT_01"
                            ng-disabled="FlagEditing==false">
                        <option value="">[Seleccione]</option>
                    </select>
                    <div class="caja11 msgerror CodigoRol">
                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroDocumento.RegistroCliente.CodigoRol" data-valmsg-replace="true"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-12 row">
                <div class="col-md-3 ">
                    Cliente
                </div>
                <div class="col-md-9">
                    <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.RegistroDocumento.RegistroCliente.CodigoPersona" />
                    <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                           type="text"
                           ng-enter="Enter()"
                           ng-model="$root.DatosFormulario.RegistroDocumento.RegistroCliente.RazonSocialPersona"
                           ng-disabled="true" />
                    <input type="image" ng-click="BuscarClienteRegistro_Click();" value="Buscar" src="\Images\buscar.png" class=""
                           style="border-width: 0px; width: 25px; margin-bottom: -10px;"
                           ng-disabled="FlagEditing==false">

                    <div class="caja11 msgerror RazonSocialPersona">
                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroDocumento.RegistroCliente.RazonSocialPersona" data-valmsg-replace="true"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="caja11 msgerror Objeto"></div>
            </div>
        </div>
    </form>
</div>
