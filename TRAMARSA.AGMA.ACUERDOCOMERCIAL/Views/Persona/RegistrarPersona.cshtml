@{
    Layout = null;
}

<div class="block ng-scope">
    <div ng-app="app" ng-controller="RegistroPersonaController" class="ng-scope">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraRegAcuerdoComercial.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos de la Persona</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <div class="block_content" style="width: 100%">
                    <form id="frmPersona_DatosGenerales">
                        <div class="col-md-6">
                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.CodigoPersona" />
                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.Accion" />
                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.RegistroPersona.DatosPersona.PersonaFlagEditar" />
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3">
                                    Razon Social:
                                </div>
                                <div class="col-md-9">
                                    <input class="InputTEXT_06" maxlength="150" style="height: 23px; width: 280px;"
                                        type="text" ng-enter="Enter()" ng-disabled="FlagEditing==false"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.RazonSocialPersona" />
                                    <div class="caja11 msgerror RazonSocialPersona">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.RazonSocialPersona" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3 ">
                                    Tipo Documento:
                                </div>
                                <div class="col-md-9">
                                    <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.CodigoTipoDocumento" />
                                    <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                        type="text"
                                        ng-enter="Enter()"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.NombreTipoDocumento"
                                        ng-disabled="true" />
                                    <input type="image" ng-click="BuscarTipoDocumentoRegistro_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                        style="border-width: 0px; width: 25px; margin-bottom: -10px;"
                                        ng-disabled="FlagEditing==false">

                                    <div class="caja11 msgerror NombreTipoDocumento">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.NombreTipoDocumento" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3">
                                    Numero de Documento:
                                </div>
                                <div class="col-md-9">
                                    <input id="NumeroDocumentoPersona" class="InputTEXT_06 solonumeros" maxlength="11" style="height: 23px; width: 150px;"
                                        type="text" ng-enter="Enter()"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.NumeroDocumentoPersona"
                                        ng-disabled="FlagEditing==false" />
                                    <div class="caja11 msgerror NumeroDocumentoPersona">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.NumeroDocumentoPersona" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3 ">
                                    Pais:
                                </div>
                                <div class="col-md-9">
                                    <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.CodigoPais" />
                                    <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                        type="text"
                                        ng-enter="Enter()"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.NombrePais"
                                        ng-disabled="true" />
                                    <input type="image" ng-click="BuscarPaisRegistro_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                        style="border-width: 0px; width: 25px; margin-bottom: -10px;"
                                        ng-disabled="FlagEditing==false">

                                    <div class="caja11 msgerror NombrePais">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.NombrePais" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3">
                                    Direccion:
                                </div>
                                <div class="col-md-9">
                                    <input class="InputTEXT_06" maxlength="250" style="height: 23px; width: 280px;"
                                        type="text" ng-enter="Enter()"
                                        ng-disabled="FlagEditing==false"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.DireccionPersona" />
                                    <div class="caja11 msgerror DireccionPersona">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.DireccionPersona" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3">
                                    Telefono:
                                </div>
                                <div class="col-md-9">
                                    <input class="InputTEXT_06" maxlength="150" style="height: 23px; width: 150px;"
                                        type="text" ng-enter="Enter()"
                                        ng-disabled="FlagEditing==false"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.TelefonoPersona" />
                                    <div class="caja11 msgerror TelefonoPersona">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.TelefonoPersona" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3">
                                    Email:
                                </div>
                                <div class="col-md-9">
                                    <input class="InputTEXT_06" maxlength="150" style="height: 23px; width: 280px;"
                                        type="text" ng-enter="Enter()"
                                        ng-disabled="FlagEditing==false"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.EmailPersona" />
                                    <div class="caja11 msgerror EmailPersona">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.EmailPersona" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3">
                                    Contacto:
                                </div>
                                <div class="col-md-9">
                                    <input class="InputTEXT_06" maxlength="80" style="height: 23px; width: 280px;"
                                        type="text" ng-enter="Enter()"
                                        ng-disabled="FlagEditing==false"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.ContactoPersona" />
                                    <div class="caja11 msgerror ContactoPersona">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.ContactoPersona" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 row rowAlto2">
                                <div class="col-md-3">
                                    Web:
                                </div>
                                <div class="col-md-9">
                                    <input class="InputTEXT_06" maxlength="80" style="height: 23px; width: 280px;"
                                        type="text" ng-enter="Enter()"
                                        ng-disabled="FlagEditing==false"
                                        ng-model="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.WebPersona" />
                                    <div class="caja11 msgerror WebPersona">
                                        <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.RegistroPersona.DatosRegistroPersona.WebPersona" data-valmsg-replace="true"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div style="margin-right: 2px;">
                                <div class="col-md-12 row" style="margin-top: 5%; margin-left: 0.3%;">
                                    <div class="block_cab block_cab_active" style="width: 100%">
                                        <h2>
                                            <span>Roles</span>
                                        </h2>
                                        <h3></h3>
                                        <div class="acordeon">
                                        </div>
                                    </div>
                                    <div id="ContentListadoListaRoles" class="block_content" style="width: 100%">
                                        <div class="form_section" style="width: 100%">
                                            <div class='forms2'>
                                                <div class='table-responsive' style="overflow: hidden">
                                                    <ng-jqgrid config="config"
                                                        addnew="true"
                                                        modeldata="$root.DatosFormulario.RegistroPersona.ListaRol"
                                                        gridid="grillaListaRoles"
                                                        pagerid="grillaListaRoles_pager"
                                                        api="gridapiListaRoles"
                                                        flgocultarbarrapaginado="1"
                                                        idgrilla="2ADD56F7-5319-442F-98A8-E5AA9FA5A867"
                                                        altogrilla="365px"
                                                        idmarcarprimerresgistro="0" url=""
                                                        flgcargargrilla="1">
                                                        <table id="grillaListaRoles"></table>
                                                        <div id="grillaListaRoles_pager"></div>
                                                   </ng-jqgrid>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="caja11 msgerror listaRol">
                                        <span class="field-validation-valid msgerror" data-valmsg-replpersona="true"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div>
                    </div>
                    <div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
