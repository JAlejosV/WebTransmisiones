                                            </select>

                                        </div>
                                    </div>



                                </div>

                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- FILTROS BUSQUEDA -->
        <!-- RESULTADO BUSQUEDA -->
        <div id="seccion-3">
            <div style="width:100%;float:left">

                <div class="block_cab block_cab_active">
                    <h2>
                        <span>RESULTADO DE BUSQUEDA</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <div id="ContentListado6b6c4707-d1aa-4a21-be0e-b9fcdbd8bfb6" class="block_content">
                    <div class="form_section" style="width:100%">

                        <div class="forms2">
