@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="ConsultaNotificacionIntegracionController">
        <!-- BOTONERA -->
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_Botonera_Buscar_Nuevo_Limpiar_Salir.cshtml")
        </div>
        <!-- BOTONERA -->
        <!-- FILTROS BUSQUEDA -->
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
