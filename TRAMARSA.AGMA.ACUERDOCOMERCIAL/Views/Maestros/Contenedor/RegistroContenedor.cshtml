@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroContenedorController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Contenedor</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarContenedorFrm" name="AdministrarContenedorFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Numero de Contenedor
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="11"
                                    ng-model="$root.DatosFormulario.AdministrarContenedor.RegistroDatos.NumeroContenedor" />
                                <div class="caja11 msgerror NumeroContenedor">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarContenedor.RegistroDatos.NumeroContenedor" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Tara
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01 solodecimal"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarContenedor.RegistroDatos.TaraContenedor" />
                                <div class="caja11 msgerror TaraContenedor">
                                    <span class="field-validation-TaraContenedor msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarContenedor.RegistroDatos.TaraContenedor" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Tipo Contenedor
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarContenedor.RegistroDatos.CodigoTipoContenedor" />
                                <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                    type="text"
                                    ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarContenedor.RegistroDatos.NombreTipoContenedor"
                                    ng-disabled="true" />
                                <input type="image" ng-click="BuscarTipoContenedorRegistro_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                    style="border-width: 0px; width: 25px; margin-bottom: -10px;">

                                <div class="caja11 msgerror NombreTipoContenedor">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarContenedor.NombreTipoContenedor" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
