@{
    Layout = null;
}

<div class="block" style="">

    <div ng-app="app" ng-controller="AdministrarContenedorController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraBusquedaAcuerdoComercial.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>ADMINISTRAR CONTENEDOR</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarContenedorFrm" name="AdministrarContenedorFrm">
                    <div class="block_content" style="width: 100%">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Numero de Contenedor
                                        </div>
                                        <div class="col-md-8">
                                            <input class="InputTEXT_05"
                                                type="text" ng-enter="Enter()" maxlength="11" 
                                                ng-model="$root.DatosFormulario.AdministrarContenedor.Filtro.NumeroContenedor" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Tara
                                        </div>
                                        <div class="col-md-8">
                                            <input class="InputTEXT_01 solodecimal"
                                                type="text" ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.AdministrarContenedor.Filtro.TaraContenedor" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Tipo
                                        </div>
                                        <div class="col-md-8">
                                            <input class="InputTEXT_01" maxlength="2" 
                                                type="text" ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.AdministrarContenedor.Filtro.CodTipoContenedor" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Tamanio
                                        </div>
                                        <div class="col-md-8">
                                            <input class="InputTEXT_01 solonumeros" maxlength="2" 
                                                type="text" ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.AdministrarContenedor.Filtro.TamanioTipoContenedor" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Tipo Contenedor
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarContenedor.Filtro.CodigoTipoContenedor" />
                                            <input class="InputTEXT_05" style="height: 23px; width: 280px;"
                                                type="text"
                                                ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.AdministrarContenedor.Datos.NombreTipoContenedor"
                                                ng-disabled="true" />
                                            <input type="image" ng-click="BuscarTipoContenedor_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                                style="border-width: 0px; width: 25px; margin-bottom: -10px;">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div id="seccion-3">
            <div style="width: 100%; float: left">

                <div class="block_cab block_cab_active">
                    <h2>
                        <span>RESULTADO DE BUSQUEDA</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <div id="ContentListado1a0b5f06-bb91-4713-aa00-966ef96a1014" class="block_content">
                    <div class="form_section" style="width: 100%">

                        <div class="forms2">
                            <div class="table-responsive" style="overflow: hidden">

                                <ng-jqgrid config="config"
                                    addnew="true"
                                    modeldata="$root.DatosFormulario.AdministrarContenedor.ListaMaestroContenedor"
                                    gridid="grillaListaMaestroContenedor"
                                    pagerid="grillaListaMaestroContenedor_pager"
                                    api="gridapigrillaListaMaestroContenedor"
                                    flgocultarbarrapaginado="0"
                                    idgrilla="1a0b5f06-bb91-4713-aa00-966ef96a1014"
                                    titulosinresultados=""
                                    ocultarexportar="0"
                                    altogrilla="150px"
                                    idmarcarprimerresgistro="0"
                                    ngclickagregar=""
                                    urllistar="/Contenedor/ConsultaContenedor"
                                    flgcargargrilla="0">
                                    <table id="grillaListaMaestroContenedor"></table>
                                    <div id="grillaListaMaestroContenedor_pager"></div>
                                </ng-jqgrid>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
