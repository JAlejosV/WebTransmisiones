@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroNumeroIMOController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Numero IMO</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarNumeroIMOFrm" name="AdministrarNumeroIMOFrm">
                    <div class="block_content" style="width: 100%">
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Numero IMO
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="255"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarNumeroIMO.RegistroDatos.NombreNumeroIMO" />
                                <div class="caja11 msgerror NombreNumeroIMO">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarNumeroIMO.RegistroDatos.NombreNumeroIMO" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Numero IMO
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05 solonumeros"
                                    type="text" ng-enter="Enter()" maxlength="4"
                                    ng-model="$root.DatosFormulario.AdministrarNumeroIMO.RegistroDatos.NumberIMO" />
                                <div class="caja11 msgerror NumberIMO">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarNumeroIMO.RegistroDatos.NumberIMO" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Pagina Numero IMO
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05 solonumeros"
                                    type="text" ng-enter="Enter()" maxlength="4"
                                    ng-model="$root.DatosFormulario.AdministrarNumeroIMO.RegistroDatos.PaginaNumeroIMO" />
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Clase IMO
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarNumeroIMO.RegistroDatos.CodigoClaseIMO" />
                                <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                    type="text"
                                    ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarNumeroIMO.RegistroDatos.NombreClaseIMO"
                                    ng-disabled="true" />
                                <input type="image" ng-click="BuscarClaseIMORegistro_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                    style="border-width: 0px; width: 25px; margin-bottom: -10px;">

                                <div class="caja11 msgerror NombreClaseIMO">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarNumeroIMO.NombreClaseIMO" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
