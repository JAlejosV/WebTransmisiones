@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroAduanaController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Aduana</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarAduanaFrm" name="AdministrarAduanaFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Sunat
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="5" onkeyup="mayus(this);"
                                    ng-model="$root.DatosFormulario.AdministrarAduana.RegistroDatos.CodigoAduanaSunat" />
                                <div class="caja11 msgerror CodigoAduanaSunat">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarAduana.RegistroDatos.CodigoAduanaSunat" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Aduana
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="150"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarAduana.RegistroDatos.NombreAduana" />
                                <div class="caja11 msgerror NombreAduana">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarAduana.RegistroDatos.NombreAduana" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Puerto
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarAduana.RegistroDatos.CodigoPuerto" />
                                <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                    type="text"
                                    ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarAduana.RegistroDatos.NombrePuerto"
                                    ng-disabled="true" />
                                <input type="image" ng-click="BuscarPuertoRegistro_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                    style="border-width: 0px; width: 25px; margin-bottom: -10px;">

                                <div class="caja11 msgerror NombrePuerto">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarAduana.NombrePuerto" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Via de Transporte
                            </div>
                            <div class="col-md-8">
                                <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarAduana.RegistroDatos.CodigoViaTransporte" />
                                <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                    type="text"
                                    ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarAduana.RegistroDatos.NombreViaTransporte"
                                    ng-disabled="true" />
                                <input type="image" ng-click="BuscarViaTransporteRegistro_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                    style="border-width: 0px; width: 25px; margin-bottom: -10px;">

                                <div class="caja11 msgerror NombreViaTransporte">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarAduana.NombreViaTransporte" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
