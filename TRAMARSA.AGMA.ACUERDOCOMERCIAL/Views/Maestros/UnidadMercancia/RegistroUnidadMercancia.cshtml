@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroUnidadMercanciaController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Unidad de Mercancia</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarUnidadMercanciaFrm" name="AdministrarUnidadMercanciaFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Sunat
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="2"
                                    ng-model="$root.DatosFormulario.AdministrarUnidadMercancia.RegistroDatos.CodigoUnidadMercanciaSunat" />
                                <div class="caja11 msgerror CodigoUnidadMercanciaSunat">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarUnidadMercancia.RegistroDatos.CodigoUnidadMercanciaSunat" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Unidad de Mercancia
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="100"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarUnidadMercancia.RegistroDatos.NombreUnidadMercancia" />
                                <div class="caja11 msgerror NombreUnidadMercancia">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarUnidadMercancia.RegistroDatos.NombreUnidadMercancia" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                         <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Aduana
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="2"
                                    ng-model="$root.DatosFormulario.AdministrarUnidadMercancia.RegistroDatos.CodigoAduanaUnidadMercancia" />
                                <div class="caja11 msgerror CodigoAduanaUnidadMercancia">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarUnidadMercancia.RegistroDatos.CodigoAduanaUnidadMercancia" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
