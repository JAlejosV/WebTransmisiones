@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroTemperaturaController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Temperatura</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarTemperaturaFrm" name="AdministrarTemperaturaFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Sunat
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="3"
                                    ng-model="$root.DatosFormulario.AdministrarTemperatura.RegistroDatos.CodigoTemperaturaSunat" />
                                <div class="caja11 msgerror CodigoTemperaturaSunat">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarTemperatura.RegistroDatos.CodigoTemperaturaSunat" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Temperatura
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="50"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarTemperatura.RegistroDatos.NombreTemperatura" />
                                <div class="caja11 msgerror NombreTemperatura">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarTemperatura.RegistroDatos.NombreTemperatura" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
