@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroCondicionTransporteController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Condicion de Transporte</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarCondicionTransporteFrm" name="AdministrarCondicionTransporteFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Sunat
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="3"
                                    ng-model="$root.DatosFormulario.AdministrarCondicionTransporte.RegistroDatos.CodigoCondicionTransporteSunat" />
                                <div class="caja11 msgerror CodigoCondicionTransporteSunat">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarCondicionTransporte.RegistroDatos.CodigoCondicionTransporteSunat" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Condicion de Transporte
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="50"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarCondicionTransporte.RegistroDatos.NombreCondicionTransporte" />
                                <div class="caja11 msgerror NombreCondicionTransporte">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarCondicionTransporte.RegistroDatos.NombreCondicionTransporte" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                         <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Aduana
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05 solonumeros"
                                    type="text" ng-enter="Enter()" maxlength="2"
                                    ng-model="$root.DatosFormulario.AdministrarCondicionTransporte.RegistroDatos.CodigoAduanaCondicionTransporte" />
                                <div class="caja11 msgerror CodigoAduanaCondicionTransporte">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarCondicionTransporte.RegistroDatos.CodigoAduanaCondicionTransporte" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
