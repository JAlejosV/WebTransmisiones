@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroRequerimientoServicioController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Requerimiento de Servicio</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarRequerimientoServicioFrm" name="AdministrarRequerimientoServicioFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Sunat
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="2"
                                    ng-model="$root.DatosFormulario.AdministrarRequerimientoServicio.RegistroDatos.CodigoRequerimientoServicioSunat" />
                                <div class="caja11 msgerror CodigoRequerimientoServicioSunat">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarRequerimientoServicio.RegistroDatos.CodigoRequerimientoServicioSunat" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Requerimiento de Servicio
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="80"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarRequerimientoServicio.RegistroDatos.NombreRequerimientoServicio" />
                                <div class="caja11 msgerror NombreRequerimientoServicio">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarRequerimientoServicio.RegistroDatos.NombreRequerimientoServicio" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
