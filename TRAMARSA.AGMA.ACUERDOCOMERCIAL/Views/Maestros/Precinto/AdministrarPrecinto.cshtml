@{
    Layout = null;
}

<div class="block" style="">

    <div ng-app="app" ng-controller="AdministrarPrecintoController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraBusquedaAcuerdoComercial.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>ADMINISTRAR PRECINTO</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarPrecintoFrm" name="AdministrarPrecintoFrm">
                    <div class="block_content" style="width: 100%">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Numero de Precinto
                                        </div>
                                        <div class="col-md-8">
                                            <input class="InputTEXT_05"
                                                type="text" ng-enter="Enter()" maxlength="15"
                                                ng-model="$root.DatosFormulario.AdministrarPrecinto.Filtro.NumeroPrecinto" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Condicion de Precinto
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarPrecinto.Filtro.CodigoCondicionPrecinto" />
                                            <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                                type="text"
                                                ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.AdministrarPrecinto.Datos.NombreCondicionPrecinto"
                                                ng-disabled="true" />
                                            <input type="image" ng-click="BuscarCondicionPrecinto_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                                style="border-width: 0px; width: 25px; margin-bottom: -10px;">
                                        </div>
                                    </div>
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Entidad de Precinto
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarPrecinto.Filtro.EntidadPrecinto" />
                                            <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                                type="text"
                                                ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.AdministrarPrecinto.Datos.NombreEntidadPrecinto"
                                                ng-disabled="true" />
                                            <input type="image" ng-click="BuscarEntidadPrecinto_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                                style="border-width: 0px; width: 25px; margin-bottom: -10px;">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div id="seccion-3">
            <div style="width: 100%; float: left">

                <div class="block_cab block_cab_active">
                    <h2>
                        <span>RESULTADO DE BUSQUEDA</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <div id="ContentListado1a0b5f06-bb91-4713-aa00-966ef96a1000" class="block_content">
                    <div class="form_section" style="width: 100%">

                        <div class="forms2">
                            <div class="table-responsive" style="overflow: hidden">

                                <ng-jqgrid config="config"
                                    addnew="true"
                                    modeldata="$root.DatosFormulario.AdministrarPrecinto.ListaMaestroPrecinto"
                                    gridid="grillaListaMaestroPrecinto"
                                    pagerid="grillaListaMaestroPrecinto_pager"
                                    api="gridapigrillaListaMaestroPrecinto"
                                    flgocultarbarrapaginado="0"
                                    idgrilla="1a0b5f06-bb91-4713-aa00-966ef96a1000"
                                    titulosinresultados=""
                                    ocultarexportar="0"
                                    altogrilla="150px"
                                    idmarcarprimerresgistro="0"
                                    ngclickagregar=""
                                    urllistar="/Precinto/ConsultaPrecinto"
                                    flgcargargrilla="0">
                                    <table id="grillaListaMaestroPrecinto"></table>
                                    <div id="grillaListaMaestroPrecinto_pager"></div>

                                </ng-jqgrid>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
