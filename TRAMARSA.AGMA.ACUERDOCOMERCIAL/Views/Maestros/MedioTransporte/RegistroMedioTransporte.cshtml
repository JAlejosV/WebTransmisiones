@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroMedioTransporteController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Medio de Transporte</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarMedioTransporteFrm" name="AdministrarMedioTransporteFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Sunat
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05 solonumeros" maxlength="2"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarMedioTransporte.RegistroDatos.CodigoMedioTransporteSunat" />
                                <div class="caja11 msgerror CodigoMedioTransporteSunat">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarMedioTransporte.RegistroDatos.CodigoMedioTransporteSunat" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Medio de Transporte
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="100"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarMedioTransporte.RegistroDatos.NombreMedioTransporte" />
                                <div class="caja11 msgerror NombreMedioTransporte">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarMedioTransporte.RegistroDatos.NombreMedioTransporte" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
