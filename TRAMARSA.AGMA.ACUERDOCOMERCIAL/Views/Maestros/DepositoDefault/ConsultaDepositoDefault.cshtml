                                               flgocultarbarrapaginado="0"
                                               idgrilla="17ed1f59-3645-4fee-8380-f04ce247e89c"
                                               titulosinresultados=""
                                               ocultarexportar="0"
                                               altogrilla="150px"
                                               idmarcarprimerresgistro="0"
                                               ngclickagregar=""
                                               urllistar="/DepositoDefault/ConsultarDepositoDefault"
                                               flgcargargrilla="0">
                                        <table id="grillaListaDepositoDefault"></table>
                                        <div id="grillaListaDepositoDefault_pager"></div>
                                    </ng-jqgrid>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        <!-- RESULTADO BUSQUEDA -->
    </div>
</div>
