@{
    Layout = null;
}

<div class="block" style="">
    <div ng-app="app" ng-controller="RegistroClaseIMOController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraGrabarMaestro.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Datos Clase IMO</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarClaseIMOFrm" name="AdministrarClaseIMOFrm">
                    <div class="block_content" style="width: 100%">

                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Codigo Sunat
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_05"
                                    type="text" ng-enter="Enter()" maxlength="4"
                                    ng-model="$root.DatosFormulario.AdministrarClaseIMO.RegistroDatos.CodigoClaseIMOSunat" />
                                <div class="caja11 msgerror CodigoClaseIMOSunat">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarClaseIMO.RegistroDatos.CodigoClaseIMOSunat" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 row">
                            <div class="col-md-4">
                                Nombre Clase IMO
                            </div>
                            <div class="col-md-8">
                                <input class="InputTEXT_01" maxlength="255"
                                    type="text" ng-enter="Enter()"
                                    ng-model="$root.DatosFormulario.AdministrarClaseIMO.RegistroDatos.NombreClaseIMO" />
                                <div class="caja11 msgerror NombreClaseIMO">
                                    <span class="field-validation-valid msgerror" data-valmsg-for="$root.DatosFormulario.AdministrarClaseIMO.RegistroDatos.NombreClaseIMO" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
