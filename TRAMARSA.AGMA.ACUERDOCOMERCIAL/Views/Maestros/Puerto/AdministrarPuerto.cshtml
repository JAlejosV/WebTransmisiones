@{
    Layout = null;
}

<div class="block" style="">

    <div ng-app="app" ng-controller="AdministrarPuertoController">
        <div id="seccion-1">
            @Html.Partial("~/Views/Botonera/AC_BotoneraBusquedaAcuerdoComercial.cshtml")
        </div>
        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>ADMINISTRAR PUERTO</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="AdministrarPuertoFrm" name="AdministrarPuertoFrm">
                    <div class="block_content" style="width: 100%">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Codigo Sunat
                                        </div>
                                        <div class="col-md-8">
                                            <input class="InputTEXT_05"
                                                type="text" ng-enter="Enter()" maxlength="5" onkeyup="mayus(this);"
                                                ng-model="$root.DatosFormulario.AdministrarPuerto.Filtro.CodigoPuertoSunat" />
                                        </div>
                                    </div>
                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Nombre Puerto
                                        </div>
                                        <div class="col-md-8">
                                            <input class="InputTEXT_01"
                                                type="text" ng-enter="Enter()" maxlength="100"
                                                ng-model="$root.DatosFormulario.AdministrarPuerto.Filtro.NombrePuerto" />
                                        </div>
                                    </div>

                                    <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Nombre Pais
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.AdministrarPuerto.Filtro.CodigoPais" />
                                            <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                                type="text"
                                                ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.AdministrarPuerto.Datos.NombrePais"
                                                ng-disabled="true" />
                                            <input type="image" ng-click="BuscarPais_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                                style="border-width: 0px; width: 25px; margin-bottom: -10px;">
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div id="seccion-3">
            <div style="width: 100%; float: left">

                <div class="block_cab block_cab_active">
                    <h2>
                        <span>RESULTADO DE BUSQUEDA</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <div id="ContentListado1a0b5f06-bb91-4713-aa00-966ef96ad985" class="block_content">
                    <div class="form_section" style="width: 100%">

                        <div class="forms2">
                            <div class="table-responsive" style="overflow: hidden">

                                <ng-jqgrid config="config"
                                    addnew="true"
                                    modeldata="$root.DatosFormulario.AdministrarPuerto.ListaMaestroPuerto"
                                    gridid="grillaListaMaestroPuerto"
                                    pagerid="grillaListaMaestroPuerto_pager"
                                    api="gridapigrillaListaMaestroPuerto"
                                    flgocultarbarrapaginado="0"
                                    idgrilla="1a0b5f06-bb91-4713-aa00-966ef96ad985"
                                    titulosinresultados=""
                                    ocultarexportar="0"
                                    altogrilla="150px"
                                    idmarcarprimerresgistro="0"
                                    ngclickagregar=""
                                    urllistar="/Puerto/ConsultaPuerto"
                                    flgcargargrilla="0">
                                    <table id="grillaListaMaestroPuerto"></table>
                                    <div id="grillaListaMaestroPuerto_pager"></div>

                                </ng-jqgrid>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
