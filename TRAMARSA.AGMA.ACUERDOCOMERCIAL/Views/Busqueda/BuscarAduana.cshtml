@{
    Layout = null;
}

<div class="block ng-scope" style="max-width: 800px;">
    <div ng-app="app" ng-controller="BuscarAduanaController" class="ng-scope">
        @Html.Partial("~/Views/Botonera/BotoneraBusquedaClásicaconBotonSalir.cshtml")

        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Administración de Aduana</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="BusquedaAduanaFrm" name="BusquedaAduanaFrm">
                    <div class="block_content" style="width: 100%">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-12 row">
                                    <div class="col-md-4">
                                        Codigo Aduana Sunat
                                    </div>
                                    <div class="col-md-8">
                                        <input class="InputTEXT_05 solonumeros"
                                            type="text" ng-enter="Enter()" maxlength="4"
                                            ng-model="$root.DatosFormulario.FiltrosBusquedaAduana.Filtro.CodigoAduanaSunat" />
                                    </div>
                                </div>
                                <div class="col-md-12 row">
                                    <div class="col-md-4">
                                        Nombre Aduana
                                    </div>
                                    <div class="col-md-8">
                                        <input class="InputTEXT_01"
                                            type="text" ng-enter="Enter()" maxlength="150"
                                            ng-model="$root.DatosFormulario.FiltrosBusquedaAduana.Filtro.NombreAduana" />
                                    </div>
                                </div>
                                <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Puerto
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.FiltrosBusquedaAduana.Filtro.CodigoPuerto" />
                                            <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                                type="text"
                                                ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.FiltrosBusquedaAduana.Filtro.NombrePuerto"
                                                ng-disabled="true" />
                                            <input type="image" ng-click="BuscarPuerto_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                                style="border-width: 0px; width: 25px; margin-bottom: -10px;">
                                        </div>
                                    </div>
                                <div class="col-md-12 row">
                                        <div class="col-md-4">
                                            Via de Transporte
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.FiltrosBusquedaAduana.Filtro.CodigoViaTransporte" />
                                            <input class="InputTEXT_06" style="height: 23px; width: 280px;"
                                                type="text"
                                                ng-enter="Enter()"
                                                ng-model="$root.DatosFormulario.FiltrosBusquedaAduana.Filtro.NombreViaTransporte"
                                                ng-disabled="true" />
                                            <input type="image" ng-click="BuscarViaTransporte_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                                style="border-width: 0px; width: 25px; margin-bottom: -10px;">
                                        </div>
                                    </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="caja11 msgerror Objeto"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div id="seccion-3">
                <div style="width: 100%; float: left">
                    <div class="block_cab block_cab_active">
                        <h2>
                            <span>RESULTADO DE BUSQUEDA</span>
                        </h2>
                        <h3></h3>
                        <div class="acordeon">
                        </div>
                    </div>
                    <div id="ContentListadof478bbe4-1dd1-4f5d-a484-fb4d7d030177" class="block_content">
                        <div class="form_section" style="width: 100%">
                            <div class="forms2">
                                <div class="table-responsive" style="overflow: hidden">
                                    <ng-jqgrid config="config"
                                        addnew="true"
                                        modeldata="$root.DatosFormulario.grillaListaAduana"
                                        gridid="grillaListaAduana"
                                        pagerid="grillaListaAduana_pager"
                                        api="gridapigrillaListaAduana"
                                        flgocultarbarrapaginado="0"
                                        idgrilla="f478bbe4-1dd1-4f5d-a484-fb4d7d030177"
                                        titulosinresultados=""
                                        ocultarexportar="0"
                                        altogrilla="200px"
                                        idmarcarprimerresgistro="0"
                                        ngclickagregar=""
                                        urllistar="/Aduana/ConsultaAduana"
                                        flgcargargrilla="1">
                                        <table id="grillaListaAduana"></table>
                                        <div id="grillaListaAduana_pager"></div>
                                    </ng-jqgrid>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
