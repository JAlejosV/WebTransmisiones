@{
    Layout = null;
}

<div class="block ng-scope" style="max-width: 800px;">
    <div ng-app="app" ng-controller="BuscarNumeroIMOController" class="ng-scope">
        @Html.Partial("~/Views/Botonera/BotoneraBusquedaClásicaconBotonSalir.cshtml")

        <div id="seccion-2">
            <div>
                <div class="block_cab block_cab_active">
                    <h2>
                        <span>Administración de Numero IMO</span>
                    </h2>
                    <h3></h3>
                    <div class="acordeon">
                    </div>
                </div>
                <form id="BusquedaNumeroIMOFrm" name="BusquedaNumeroIMOFrm">
                    <div class="block_content" style="width: 100%">
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-4">
                                    Numero Numero IMO
                                </div>
                                <div class="col-md-8">
                                    <input class="InputTEXT_05"
                                        type="text" ng-enter="Enter()" maxlength="255"
                                        ng-model="$root.DatosFormulario.FiltrosBusquedaNumeroIMO.Filtro.NombreNumeroIMO" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                                    Numero IMO
                                </div>
                                <div class="col-md-8">
                                    <input class="InputTEXT_01 solonumeros"
                                        type="text" ng-enter="Enter()" maxlength="4"
                                        ng-model="$root.DatosFormulario.FiltrosBusquedaNumeroIMO.Filtro.NumberIMO" />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-4">
                                    Pagina Numero IMO
                                </div>
                                <div class="col-md-8">
                                    <input class="InputTEXT_01 solonumeros"
                                        type="text" ng-enter="Enter()" maxlength="4"
                                        ng-model="$root.DatosFormulario.FiltrosBusquedaNumeroIMO.Filtro.PaginaNumeroIMO" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                                    Clase IMO
                                </div>
                                <div class="col-md-8">
                                    <input type="text" class="ng-hide" ng-model="$root.DatosFormulario.FiltrosBusquedaNumeroIMO.Filtro.CodigoClaseIMO" />
                                    <input class="InputTEXT_05" style="height: 23px; width: 280px;"
                                        type="text"
                                        ng-enter="Enter()"
                                        ng-model="$root.DatosFormulario.FiltrosBusquedaNumeroIMO.Filtro.NombreClaseIMO"
                                        ng-disabled="true" />
                                    <input type="image" ng-click="BuscarClaseIMO_Click();" value="Buscar" src="\Images\buscar.png" class=""
                                        style="border-width: 0px; width: 25px; margin-bottom: -10px;">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="caja11 msgerror Objeto"></div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div id="seccion-3">
                <div style="width: 100%; float: left">
                    <div class="block_cab block_cab_active">
                        <h2>
                            <span>RESULTADO DE BUSQUEDA</span>
                        </h2>
                        <h3></h3>
                        <div class="acordeon">
                        </div>
                    </div>
                    <div id="ContentListadof478bbe4-1dd1-4f5d-a484-fb4d7d030206" class="block_content">
                        <div class="form_section" style="width: 100%">
                            <div class="forms2">
                                <div class="table-responsive" style="overflow: hidden">
                                    <ng-jqgrid config="config"
                                        addnew="true"
                                        modeldata="$root.DatosFormulario.grillaListaNumeroIMO"
                                        gridid="grillaListaNumeroIMO"
                                        pagerid="grillaListaNumeroIMO_pager"
                                        api="gridapigrillaListaNumeroIMO"
                                        flgocultarbarrapaginado="0"
                                        idgrilla="f478bbe4-1dd1-4f5d-a484-fb4d7d030206"
                                        titulosinresultados=""
                                        ocultarexportar="0"
                                        altogrilla="200px"
                                        idmarcarprimerresgistro="0"
                                        ngclickagregar=""
                                        urllistar="/NumeroIMO/ConsultaNumeroIMO"
                                        flgcargargrilla="1">
                                        <table id="grillaListaNumeroIMO"></table>
                                        <div id="grillaListaNumeroIMO_pager"></div>
                                    </ng-jqgrid>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
